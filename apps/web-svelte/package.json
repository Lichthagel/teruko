{
  "name": "web-svelte",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=21.0.0"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "clean": "rm -rf .svelte-kit build"
  },
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "{projectRoot}/.svelte-kit",
          "{projectRoot}/build"
        ]
      }
    },
    "includedScripts": [
      "dev",
      "build",
      "preview",
      "lint",
      "format"
    ]
  },
  "dependencies": {
    "@urql/svelte": "^4.2.1",
    "client-css": "workspace:*",
    "client-graphql": "workspace:*",
    "client-stores": "workspace:*",
    "drizzle-orm": "^0.41.0",
    "lucide-svelte": "^0.485.0",
    "models": "workspace:*",
    "server-db": "workspace:^",
    "server-env": "workspace:^",
    "server-graphql": "workspace:^",
    "sharp": "^0.33.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^5.2.9",
    "@sveltejs/kit": "^2.7.4",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@types/node": "^22.8.7",
    "graphql": "^17.0.0-alpha",
    "sass-embedded": "^1.83.0",
    "svelte": "^5.1.9",
    "svelte-check": "^4.0.5",
    "tsconfig": "workspace:*",
    "tslib": "^2.8.1",
    "typescript": "^5.6.3",
    "vite": "^6.0.0"
  }
}
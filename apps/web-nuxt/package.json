{
  "name": "web-nuxt",
  "private": true,
  "engines": {
    "node": ">=21.0.0"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "lint": "eslint .",
    "clean": "rm -rf .nuxt .output"
  },
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "{projectRoot}/.output"
        ]
      }
    },
    "includedScripts": [
      "build",
      "dev",
      "lint",
      "preview"
    ]
  },
  "dependencies": {
    "@nanostores/vue": "^1.0.0",
    "@urql/vue": "^2.0.0",
    "client-css": "workspace:*",
    "client-graphql": "workspace:*",
    "client-stores": "workspace:*",
    "drizzle-orm": "^0.45.0",
    "graphql": "^17.0.0-alpha",
    "lucide-vue-next": "^0.555.0",
    "models": "workspace:*",
    "server-db": "workspace:^",
    "server-env": "workspace:^",
    "server-graphql": "workspace:^",
    "sharp": "^0.34.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^3.0.0",
    "@types/node": "^24.0.0",
    "nuxt": "^4.0.0",
    "sass-embedded": "^1.83.0",
    "tsconfig": "workspace:*"
  }
}

{
  "extends": "tsconfig/base.json",
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "lib": ["ESNext", "DOM"],
    "useDefineForClassFields": true,
    "noImplicitReturns": true,
    "noEmit": true
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}

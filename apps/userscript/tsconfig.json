{
  "extends": "tsconfig/base.json",
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["ESNext", "DOM"],
    "noEmit": true,
    "noImplicitReturns": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
  },
  "exclude": [
    "node_modules",
    "dist",
  ]
}
